# E*TRADE API Configuration
# Copy this file to .env for local development

# Sandbox credentials (already configured in config.py)
ETRADE_SANDBOX_KEY=your_sandbox_key
ETRADE_SANDBOX_SECRET=your_sandbox_secret

# Production credentials (when ready)
ETRADE_PROD_KEY=your_production_key
ETRADE_PROD_SECRET=your_production_secret

# Environment: true for sandbox, false for production
ETRADE_USE_SANDBOX=true

# Redis URL (Railway provides this automatically)
REDIS_URL=redis://localhost:6379

# Flask secret key
FLASK_SECRET_KEY=your-random-secret-key-here
